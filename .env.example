# ========================================
# QUANT EDGE LABS - REQUIRED ENVIRONMENT VARIABLES
# ========================================
# Copy this file to .env and fill in the values
# Get these from Replit Secrets tab

# ========================================
# DATABASE (REQUIRED - App will crash without this)
# ========================================
DATABASE_URL=postgresql://username:password@host:5432/database

# ========================================
# SESSION (REQUIRED)
# ========================================
SESSION_SECRET=your-random-secret-here

# ========================================
# MARKET DATA APIs (REQUIRED for stock analysis)
# ========================================
TRADIER_API_KEY=your-tradier-api-key
TRADIER_ACCOUNT_ID=your-tradier-account-id
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key

# ========================================
# AI PROVIDERS (At least ONE required for AI analysis)
# ========================================
# Anthropic Claude (Primary)
ANTHROPIC_API_KEY=your-anthropic-key

# OpenAI GPT (Backup)
OPENAI_API_KEY=your-openai-key

# Google Gemini (FREE - 1500 req/day)
GEMINI_API_KEY=your-gemini-key

# Groq (FREE - 14,400 req/day - FASTEST)
GROQ_API_KEY=your-groq-key

# Mistral (FREE - 1B tokens/month)
MISTRAL_API_KEY=your-mistral-key

# Grok
GROK_API_KEY=your-grok-key

# ========================================
# OPTIONAL INTEGRATIONS
# ========================================
# Discord Webhooks
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/...
DISCORD_WEBHOOK_GAINS=https://discord.com/api/webhooks/...
DISCORD_WEBHOOK_CHARTANALYSIS=https://discord.com/api/webhooks/...
DISCORD_WEBHOOK_FUTURE_TRADES=https://discord.com/api/webhooks/...
DISCORD_WEBHOOK_QUANTFLOOR=https://discord.com/api/webhooks/...

# Email (Resend)
RESEND_API_KEY=your-resend-key

# Notion
NOTION_API_KEY=your-notion-key
NOTION_DATABASE_ID=your-notion-db-id

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Admin Access
ADMIN_ACCESS_CODE=your-admin-code
ADMIN_PASSWORD=your-admin-password

# ========================================
# DATA PROVIDERS (Optional but recommended)
# ========================================
DATABENTO_API_KEY=your-databento-key
OPENROUTER_API_KEY=your-openrouter-key
CEREBRAS_API_KEY=your-cerebras-key
